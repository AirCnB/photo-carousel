{
  "name": "photo-carousel",
  "version": "1.0.0",
  "description": "AirCnB module for viewing photos with an interactive carousel design",
  "main": ".eslintrc.js",
  "directories": {
    "example": "example"
  },
  "scripts": {
    "test": "jest --detectOpenHandles",
    "build": "webpack",
    "start": "nodemon ./server/app.js",
    "eslint": "eslint . --ext .jsx --ext .js"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/setupTests.js",
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    },
    "collectCoverage": true,
    "testPathIgnorePatterns": [
      "<rootDir>/database/",
      "<rootDir>/node_modules/"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AirCnB/photo-carousel.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AirCnB/photo-carousel/issues"
  },
  "homepage": "https://github.com/AirCnB/photo-carousel#readme",
  "devDependencies": {
    "babel-jest": "^23.4.2",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "jest": "^23.4.1",
    "jest-enzyme": "^6.0.2",
    "superagent": "^3.8.3",
    "supertest": "^3.1.0",
    "webpack-cli": "^3.0.8",
    "webpack-dev-middleware": "^3.1.3"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.3",
    "css-loader": "^1.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint-plugin-atom": "^0.1.2",
    "express": "^4.16.3",
    "file-system": "^2.2.2",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.4",
    "nodemon": "^1.18.3",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "style-loader": "^0.21.0",
    "webpack": "^4.16.1"
  }
}
